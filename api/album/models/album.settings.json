{
  "connection": "default",
  "collectionName": "albums",
  "info": {
    "name": "Album"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "YearRecorded": {
      "type": "integer"
    },
    "Producer": {
      "type": "string"
    },
    "IsStudioRecording": {
      "type": "boolean"
    },
    "IsLiveRecording": {
      "type": "boolean"
    },
    "track": {
      "via": "album",
      "model": "track"
    }
  }
}
