{
  "connection": "default",
  "collectionName": "tracks",
  "info": {
    "name": "Track"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Length": {
      "type": "string",
      "required": true,
      "default": "mm:ss"
    },
    "HasFilmClip": {
      "type": "boolean"
    },
    "VideoUrl": {
      "type": "string"
    }
  }
}
