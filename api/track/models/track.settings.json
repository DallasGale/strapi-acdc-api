{
  "connection": "default",
  "collectionName": "tracks",
  "info": {
    "name": "Track"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Name": {
      "type": "string",
      "private": false,
      "required": true
    },
    "WrittenBy": {
      "type": "component",
      "repeatable": true,
      "component": "person.song-writer"
    },
    "Length": {
      "type": "string"
    }
  }
}
