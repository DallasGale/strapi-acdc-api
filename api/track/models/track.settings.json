{
  "connection": "default",
  "collectionName": "tracks",
  "info": {
    "name": "Track"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "HasFilmClip": {
      "type": "boolean"
    },
    "VideoUrl": {
      "type": "string"
    },
    "SingleRelease": {
      "type": "boolean"
    },
    "Length": {
      "type": "string"
    },
    "performers": {
      "collection": "performer"
    },
    "Songwriter": {
      "type": "component",
      "repeatable": true,
      "component": "person.song-writer"
    }
  }
}
